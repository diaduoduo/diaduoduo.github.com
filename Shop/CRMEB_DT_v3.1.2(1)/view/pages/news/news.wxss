/* pages/news/news.wxss */
.navbar{position:fixed;background-color:#fff;top:0;left:0;z-index:999;width:100%; z-index: 13;}
.navbar.on{background-color:#e93323;}
.navbar.black{background-color:#232323;}
.navbar.gray{background-color:#666;}
.navbar.red{background-color:#eb5447;}
.navbar.gradual01{background-image: linear-gradient(to right,#f67837 0%,#f11d09 100%);}
.navbar .navbarH{position:relative;}
.navbar .navbarH .navbarCon{position:absolute;bottom:0;height:100rpx;width:100%;}

.arrow_icon{
  position: absolute;
  left: 30rpx;
  top:30rpx;
  width: 20rpx;
  height: 36rpx;
  padding: 10rpx;
}
.diary_banner {
  width: 750rpx;
  height: 535rpx;
}

.diary_header {
  width:690rpx;
border-bottom:1rpx solid rgba(238,238,238,1);
margin:40rpx auto 20rpx auto;
padding: 7rpx 0;
position: relative;
}

.header_img {
  width:60rpx;
  height:60rpx;
  border-radius:50%;
  float: left;
  margin-right: 25rpx;
}

.header_name {
  height: 60rpx;
  font-size: 32rpx;
  font-family:PingFang SC;
  font-weight: 400;
  color: rgba(43, 43, 43, 1);
  line-height: 60rpx;
}

.diary_title {
  font-size:34rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(43,43,43,1);
  margin-bottom: 37rpx;
}

.diray_content {
  width: 670rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height:44rpx;
  margin: 0 auto;
}

.diary_body {
  padding: 0 40rpx 40rpx;
}

.diary_data {
  margin-top:60rpx;
  width: 100%;
  height: 40rpx;
  font-size: 28rpx;
  font-family: GothamBookRegular;
  font-weight: 400;
  color: rgba(138, 138, 138, 1);
  text-align: right;
  padding-bottom: 20rpx;
}
.slide-image{
  width: 100%;
  display: block
}
.swiper-item {
  position: relative
}
.swiper_nums{
  position: absolute;
  bottom:10rpx;
  right:32rpx;
width:90rpx;
height:56rpx;
background:rgba(0,0,0,1);
opacity:0.3;
border-radius:28rpx;
text-align: center;
line-height: 56rpx;
font-size:32rpx;
font-family:PingFang SC;
font-weight:400;
color:rgba(255,255,255,1);
}
.dots{
  position: absolute;
  left: 0;
  right: 0;
  bottom: -40rpx;
  display: flex;
  justify-content: center;
}
.dots .dot{
  margin: 0 5rpx;
  width:12rpx;
  height:12rpx;
  background:rgba(0,0,0,1);
  opacity:0.3;
  border-radius:50%;
  transition: all .6s;
}
.dots .dot.active{
  background:rgba(255,41,41,1);
  opacity:0.8;
  border-radius:50%;
}
.masonry_num{
  position: absolute;
  right: 20rpx;
  top:20rpx
}

.zan_icon {
  background: url("data:image/png;base64,") no-repeat;
  background-size: 24rpx 24rpx;
  width: 24rpx;
  height: 24rpx;
  display: inline-block;
  vertical-align: middle;
}

.zan_icon_active {
  background: url("data:image/png;base64,") no-repeat;
  background-size: 24rpx 24rpx;
}

.give-num {
  display: inline-block;
  font-size: 20rpx;
  color: #666;
  margin-left: 5rpx;
  line-height: 60rpx;
}
video{
  width: 750rpx;
  height: 360rpx;
}
swiper {
  transition: all 0.1s;
}