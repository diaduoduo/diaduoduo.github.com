/* pages/find/find.wxss */
.navbar{position:fixed;background-color:#fff;top:0;left:0;z-index:999;width:100%; z-index: 13;}
.navbar.on{background-color:#e93323;}
.navbar.black{background-color:#232323;}
.navbar.gray{background-color:#666;}
.navbar.red{background-color:#eb5447;}
.navbar.gradual01{background-image: linear-gradient(to right,#f67837 0%,#f11d09 100%);}
.navbar .navbarH{position:relative;}
.navbar .navbarH .navbarCon{position:absolute;bottom:0;height:100rpx;width:100%;}


.find_list{
  overflow: hidden;
}
.find_list .find_li{
  float: left;
  font-size:34rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:#999999;
  margin-right: 35rpx;
}
.find_list .find_li.on{
  color: #3C2D44;
}
.find-hide{
  display: none;
}
.img_item {
  width: 48%;
  margin: 1%;
  display: inline-block;
  vertical-align: top;
}
/* pubu s */
.case-page {
  padding:10rpx 36rpx 0 36rpx;
  display: flex;
  justify-content: space-between;

}

.list-masonry {
  width: 328rpx;
}

.item-masonry {
  background-color: #fff;
  break-inside: avoid;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}

.list-masonry-info {
  padding: 10rpx 20rpx 0rpx 20rpx;
}

.item-masonry image {
  width: 100%;
  border-radius: 16rpx;
}

.title {
  font-size: 26rpx;
  color: #2b2b2b;
  height: 76rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

.masonry_info {
  display: flex;
  justify-content: space-between;
  height: 60rpx;
}

.masonry_user{
  width: 200rpx;
}

.masonry_info .masonry_user image {
  width: 32rpx;
  height: 32rpx;
  border-radius: 100%;
  float: left;
  margin-right: 10rpx;
  margin-top: 14rpx;
}

.masonry_info .masonry_user .masonry_name {
  color: #666;
  font-size: 20rpx;
  display: inline-block;
  width: 150rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  line-height: 60rpx;
  height: 60rpx;
}
.masonry_num{
  flex: 1;
  text-align: right
}

.zan_icon {
  background: url("data:image/png;base64,") no-repeat;
  background-size: 24rpx 24rpx;
  width: 24rpx;
  height: 24rpx;
  display: inline-block;
  vertical-align: middle;
}

.zan_icon_active {
  background: url("data:image/png;base64,") no-repeat;
  background-size: 24rpx 24rpx;
}

.give-num {
  display: inline-block;
  font-size: 20rpx;
  color: #666;
  margin-left: 5rpx;
  line-height: 60rpx;
}
.wrap-list{
  position: fixed;
  top:0;
  left: 0;
}
.fixedTop .scroll_box{
  position: fixed;
  left:0;
  width: 100%;
  margin-top: 0;
  background: #fff;
}
.swiper_box {
  margin: 30rpx 0;
  overflow: hidden;
}

.page-section {
  width: 100%;
  margin-bottom: 60rpx;
}

.page-section_center {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.page-section:last-child {
  margin-bottom: 0;
}

.page-section-gap {
  box-sizing: border-box;
  padding: 0 30rpx;
}

.page-section-spacing {
  box-sizing: border-box;
}

.page-section-title {
  font-size: 28rpx;
  color: #999;
  margin-bottom: 10rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.page-section-gap .page-section-title {
  padding-left: 0;
  padding-right: 0;
}
.scroll_box {
  position: absolute;
  top:-80rpx;
  white-space: nowrap;
  display: flex;
  height: 53rpx;
  border-bottom: #F0F0F0 solid 1rpx;
}

.scroll_box .scroll_title {
  text-align: center;
  display:inline-block;
  padding: 0 70rpx 0 20rpx;
}

.scroll_box .scroll_title {
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(14,14,14,1);
  position: relative;
  line-height: 40rpx;
  height: 40rpx;
  text-align: left;

}

.active {

  border-bottom: 8rpx solid #0E0E0E;
  padding-bottom: 6rpx;
}

.wrap-type {
  display: inline-block;

}
.fengmian{
  margin-top: 80rpx;
}